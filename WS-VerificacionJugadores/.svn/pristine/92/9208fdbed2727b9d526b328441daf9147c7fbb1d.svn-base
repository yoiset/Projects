log4j.rootCategory=DEBUG, LOGFILE

log4j.logger.es.gob=DEBUG
log4j.logger.org=WARN
log4j.logger.org.springframework=DEBUG
log4j.logger.org.apache.cxf=DEBUG
log4j.logger.org.hibernate=DEBUG

log4j.appender.LOGFILE=org.apache.log4j.RollingFileAppender
#log4j.appender.LOGFILE.File=/documentospro/logs/${dgoj.hostname}-scsp-ws.log
log4j.appender.LOGFILE.File=c:/temp/ws-dgoj.log
log4j.appender.LOGFILE.MaxFileSize=10000KB
log4j.appender.LOGFILE.MaxBackupIndex=20
log4j.appender.LOGFILE.layout=org.apache.log4j.PatternLayout
log4j.appender.LOGFILE.layout.ConversionPattern=%d{yyyy-MM-dd HH:mm:ss} [%c] %-5p - %m%n
