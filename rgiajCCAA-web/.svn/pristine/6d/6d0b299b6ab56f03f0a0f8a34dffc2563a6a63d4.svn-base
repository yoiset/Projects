#contenido #interior #central table.relativa input {padding: 0 0 0 0.2em; margin: 0; font-family: Arial, helvetica, sans-serif; font-size:1em;}
#contenido #interior #central table.relativa tfoot {color: #11515B; text-align: left; padding: 1.8em; font-weight: bold; }
#contenido #interior #central table.relativa a.consultar {color:#668024; font-size: 1em; font-weight: bold; background: #fff url("../images/consultar.gif") no-repeat; padding-left: 1.9em;}
#contenido #interior #central table.relativa a.consultar:hover {text-decoration: underline}
#contenido #interior #central table.relativa a.eliminar {color:#B90000; font-size: 1em; font-weight: bold; background: #fff url("../images/eliminar.gif") no-repeat; padding-left: 1.8em;}
#contenido #interior #central table.relativa a.eliminar:hover {text-decoration: underline}
#contenido #interior #central table.relativa input.botborra {cursor:pointer; width:auto; font-weight:bold; font-size:0.9em; color:#000; background: #fff url("../images/fondo-boton-mas.jpg") right top repeat-x; border:0.06em solid #737373; margin:0.1em 0.1em 0.1em 0.1em; text-align: center; padding:0;}


/* Pie de pagina */
#contenido #inferior {clear:both; margin:0; padding:0; height:3.75em; width:100%;}
#contenido #inferior #pie-izquierdo {float:left; margin:0.5em 0 0 0.2em; padding: 0; font-size:0.7em; width: 45em;}
#contenido #inferior #pie-izquierdo a {color:#000; text-decoration:none;}
#contenido #inferior #pie-izquierdo a:hover {color:#576C1E; text-decoration:underline;}

#contenido #inferior #pie {float:right; text-align: right; margin:0.5em 0 0 0.7em; padding: 0; }
#contenido #inferior #pie p {padding:0 0 0 0; margin: 0; font-size:0.65em; line-height:1.25em;}
#contenido #inferior #pie p images {border:0;text-align:right;}
#contenido #inferior #pie images {float:right;padding-left:0.2em;padding-right:0.5em;border:0;}
#contenido #inferior #pie a {color:#047;font-size:0.9em;margin-top:0.2em;text-decoration:underline;}
#contenido #inferior #pie a:hover {color:#800000;font-size:0.9em;text-decoration:underline;}


/* Comunes */
a {color:#000; font-weight: bold; text-decoration: none;}
a:hover {color:#F10301; text-decoration:none;border:0;}
images {border:0em;}


/*Campos con label en linea LARGO */
#contenido #interior #central #formulario fieldset label.linea-largo {clear: both; display: block; margin: 0em 0 0em 1em; padding: 0.2em 0 0.2em 0;}
#contenido #interior #central #formulario fieldset label.linea-largo span.label {clear: left; color: #000; width: 34em; text-align: left; font-family: Arial, Helvetica, sans-serif; float: left; font-size: 0.75em; padding: 0.1em 0.3em 0 0.2em;}
#contenido #interior #central #formulario fieldset label.linea-largo input {float: left; text-align: left; background-color: #fff; border: 1px #9EB6BA solid; padding-left: 0.2em; margin: 0 0 0 0.4em; font-family: Arial, Helvetica, sans-serif; font-size: 0.75em;}
#contenido #interior #central #formulario fieldset label.linea-largo .file {width: 39em; display: block;}
#contenido #interior #central #formulario fieldset label.linea-largo input.checkbox {float: left; text-align: left; background-color: #fff; border: 0px; padding-left: 0.2em; margin: 0 0 0 0.4em; font-family: Arial, Helvetica, sans-serif; font-size: 0.75em;}
#contenido #interior #central #formulario fieldset label.linea-largo select {float: left; text-align: left; background-color: #fff; border: 1px #9EB6BA solid; padding: 0; margin: 0 0 0 0.4em; font-family: Arial, Helvetica, sans-serif; font-size: 0.75em; }
#contenido #interior #central #formulario fieldset label.linea-largo textarea {width: 39em; height: 6em; float: left; text-align: left; background-color: #fff; border: 1px #9EB6BA solid; padding-left: 0.2em; margin: 0 0 0 0.4em; font-family: Arial, Helvetica, sans-serif; font-size: 0.75em;}
#contenido #interior #central #formulario fieldset label.linea-largo span.date {font-size: 0.75em; padding-left: 0.6em; font-family: Arial, Helvetica, sans-serif; }
#contenido #interior #central #formulario fieldset label.linea-largo .disabled {float: left; text-align: left; background-color: #eee; border: 1px #9EB6BA solid; padding-left: 0.2em; margin: 0 0 0 0.4em; font-family: Arial, Helvetica, sans-serif; font-size: 0.75em;}
#contenido #interior #central #formulario fieldset label.linea-largo a.link {padding-left: 0.6em; font-size: 0.75em;}


/* Para el menu tipo arbol*/
#contenido #interior #central #formulario #menu-arbol {clear: both; display: block; font-size: 0.75em; font-family: Arial, helvetica, sans-serif; list-style-image: url("../images/doct.gif"); padding: 0em 0.5em 0.5em 2.5em; list-style: inside; margin: 0.4em 0 0 2em; }
#contenido #interior #central #formulario #menu-arbol {list-style: outside;}
#contenido #interior #central #formulario #menu-arbol ul li {margin: 0 0 0 1em; padding: 0 0 0 0.5em; text-decoration: none;}
#contenido #interior #central #formulario #menu-arbol a {color: #000;}
#contenido #interior #central #formulario #menu-arbol .cAbierta {list-style-type: none; list-style-image: url("../images/cap-abierta.gif"); padding: 0em 0 0.1em 0.1em; margin: 0;}
#contenido #interior #central #formulario #menu-arbol .cCerrada {list-style-type: none; list-style-image: url("../images/cap-cerrada.gif"); padding: 0em 0 0.1em 0.1em; margin: 0;}
#contenido #interior #central #formulario #menu-arbol .menuDoc {list-style-type: none; list-style-image: url("../images/menu-doc.gif");}
#contenido #interior #central #formulario #menu-arbol ul li .tagApartado, a.tagApartado {cursor: pointer; font-weight: bold; text-decoration: none}
#contenido #interior #central #formulario #menu-arbol .tagApartado, a.tagApartado {text-decoration: underline}
#contenido #interior #central #formulario #menu-arbol form {display: inline;}
#contenido #interior #central #formulario #menu-arbol form input {cursor: pointer; font-weight: bold; font-family: Arial, helvetica, sans-serif; color:#54691E; background:#fff url("../images/icono-flecha-listado.gif") left no-repeat; border:0; padding:0.4em 0 0 1em; margin:0em 0 0 0.4em; text-align:left; width:auto;}
#contenido #interior #central #formulario #menu-arbol images {margin: 0; text-decoration: none}

.lbAction {color: #cc0000}
.lbAction a {color: #cc0000}
.lbAction a:hover {color: #cc0000}


/* radiobuttons */
#contenido #interior #central #formulcuad .tradio { display:inline; margin:0; padding:0; color: #000; font-size:100%;}
#contenido #interior #central #formulcuad .tradio input {margin:0; padding:0; float:left;}
#contenido #interior #central #formulcuad .tradio label {margin:0; padding:0; float:left; margin:-1em 0 0.3em 2em; font-size:0.75em;  width:auto;}
#contenido #interior #central #formulcuad .tradio br.clear {clear:both; height:1.5em; display:inline-block;}

.clear {clear: both; margin: 0; padding: 0;}
.clearDos {clear:both; margin:0; padding:0; line-height:3.2em;}

#contenido #interior #central #formulario div.veryeliminar {float:left; width:11em; color:#000; text-align:left; padding:0em 0em 0em 0.5em; border:1px solid #DDD; margin:-0.1em 0.2em 0 0.2em; background: #F1F1F1}
