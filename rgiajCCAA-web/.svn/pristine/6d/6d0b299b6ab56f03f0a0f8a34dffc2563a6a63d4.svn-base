#contenido #interior #login fieldset label input {float: left; text-align: left; background-color: #fff; border: 1px #809F2D solid; padding-left: 0.2em; margin: 0 0 0 0.4em; font-size: 0.75em; font-family: Arial, helvetica, sans-serif;}
#contenido #interior #login .botones-login {width:20em; margin:3em auto 0 auto; clear:both; display:block; text-align:right;}
#contenido #interior #login .botones-login .boton {cursor: pointer; width:auto; font-weight: bold; font-size:0.7em;  color:#000; background: #fff url("../images/fondo-boton.jpg") bottom repeat-x; border:0.06em solid #737373; margin:0.4em; text-align: center; padding: 0.25em 0.6em 0.25em 0.6em;}

/* MENU PRINCIPAL */
.menu ul, .menu li, .menu span, .menu a {margin: 0;padding: 0;position: relative;}
.menu {height: 40px;border-radius: 5px 5px 0 0;-moz-border-radius: 5px 5px 0 0;-webkit-border-radius: 5px 5px 0 0;background: -moz-linear-gradient(left, #cccccc 0%, #e2e2e2 60%, #ffffff 75%);background: -webkit-linear-gradient(left, #cccccc 0%, #e2e2e2 60%, #ffffff 75%);background: -webkit-gradient(linear, left, right , color-stop(0%, #cccccc), color-stop(60%, #e2e2e2), color-stop(75%, #ffffff));background: -o-linear-gradient(left, #cccccc 0%, #e2e2e2 60%, #ffffff 75%);background: -ms-linear-gradient(left, #cccccc 0%, #e2e2e2 60%, #ffffff 75%);background: linear-gradient(to rigth, #cccccc 0%, #e2e2e2 60%, #ffffff 75%);filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#cccccc', endColorstr='#ffffff',GradientType=1 );border-bottom: 2px solid #bfd67a;}
.menu:after, .menu ul:after {content: '';display: block;clear: both;}
.menu a {background: transparent;color: #ffffff;display: inline-block;font-family: Helvetica, Arial, Verdana, sans-serif;font-weight: bold;font-size: 16px;line-height: 40px;padding: 0 20px;text-decoration: none;}
.menu ul {list-style: none;}
.menu > ul {float: left;}
.menu > ul > li {float: left;}
/* Triangulito debajo de la casilla seleccionada */
.menu > ul > li:first-child > a {border-radius: 5px 0 0 0;-moz-border-radius: 5px 0 0 0;-webkit-border-radius: 5px 0 0 0;}
.menu > ul > li:last-child > a {border-radius: 0 5px 0 0;-moz-border-radius: 0 5px 0 0;-webkit-border-radius: 0 5px 0 0;}
.menu > ul > li > a.active {box-shadow: inset 0 0 3px #000000;-moz-box-shadow: inset 0 0 3px #000000;-webkit-box-shadow: inset 0 0 3px #000000;background: -moz-linear-gradient(top, #cccccc 0%, #909090 100%);background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #cccccc), color-stop(100%, #909090));background: -webkit-linear-gradient(top, #cccccc 0%, #909090 100%);background: -o-linear-gradient(top, #cccccc 0%, #909090 100%);background: -ms-linear-gradient(top, #cccccc 0%, #909090 100%);background: linear-gradient(to bottom, #cccccc 0%, #909090 100%);}
/* Sombreado de celda cuando se pone raton encima -> es igual que el de arriba, para la casilla "activa" */
.menu .has-sub {z-index: 1;}
.menu .has-sub ul {display: none;position: absolute;width: 200px;top: 100%;left: 0;}
.menu .has-sub ul li {*margin-bottom: -1px;}
.menu .has-sub ul li a {background: #EFF4DF;border-bottom: 1px dotted #cccccc;filter: none;color: black;font-weight: bold;font-size: 12px;display: block;line-height: 120%;padding: 10px;}
.menu .has-sub .has-sub ul {display: none;position: absolute;left: 100%;top: 0;}
.menu .has-sub .has-sub ul li a {background: #e6edcc;border-bottom: 1px dotted #cccccc;}
/* Nombre de la aplicacion */
.menu > p#NombreApp {float: right;width:350px;margin-top:1%;color: #BFD67A;display: inline-block;font-family: Helvetica, Arial, Verdana, sans-serif;font-weight: bold;font-style: italic;font-size: 25px;text-align: center;}
/* ----------------------------------------------------------------------------------- */

/* BREADCRUMB */
.breadcrumb{position:relative; height:30px; width:97%;}
.breadcrumb ul{position:relative; margin:0 5px 0 5px; padding:0; height:30px; background:url('../images/breadcrumbImage.png') 0px -30px repeat-x; }
.breadcrumb ul li{padding:0; margin:0; list-style:none; float:left; height:30px display:inline;}
.breadcrumb ul li a{font:12px Arial, Helvetica, sans-serif; color:rgb(0,0,0); text-decoration:none; line-height:30px; display:block; background:url('../images/breadcrumbImage.png') 100% -60px no-repeat; padding:0 17px 0 15px; outline:none;}
.breadcrumb ul li a:hover {text-decoration:underline; color:rgb(168,191,134);}
.breadcrumb ul li a.active{background:none; color:rgb(156,189,108);}
.breadcrumb ul li a.active:hover {text-decoration:none;}
.breadcrumb .left{position:absolute; top:0px; left:0px; width:10px; height:30px; display:block; z-index:1; background:url('../images/breadcrumbImage.png') 0% 0px no-repeat;}
.breadcrumb .right{position:absolute; top:0px; right:0px; width:10px; height:30px; display:block; z-index:1; background:url('../images/breadcrumbImage.png') 100% -90px no-repeat; }

