/* Login */
#contenido #interior #login {height:6em; width:22em; margin:3em auto 0.5em auto; border:0; background-color:#E5EECC; padding:1em 1em 0 1em; background:url('../images/fon-login.jpg') top center no-repeat; }
*+html #contenido #interior #login label {margin:0.2em 0.5em 0 2em;}
