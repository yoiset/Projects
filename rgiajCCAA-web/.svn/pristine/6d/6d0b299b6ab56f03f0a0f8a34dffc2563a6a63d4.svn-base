#contenido #interior #central #error-tecnico a {font-size:100%; font-weight:bold; color: #126985;}
#contenido #interior #central #error-tecnico .cerrar {clear: both; float: right; padding:0em 2em 0.4em 0; background: url("../images/ico-cerrar.gif") top right no-repeat;}
