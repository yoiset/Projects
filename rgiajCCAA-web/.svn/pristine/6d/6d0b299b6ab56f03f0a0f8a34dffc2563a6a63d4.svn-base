/* Estilos body */
body {background:#fff; color:#000; margin:0; padding:0; font-family: Arial, Verdana, Tahoma, helvetica, sans-serif; text-align:left; font-size:100%;}

/* Genericos Campos formularios */
input{font-family: Arial, Verdana, Tahoma, helvetica, sans-serif;}
select {font-family: Arial, Verdana, Tahoma, helvetica, sans-serif;}
textarea {font-family: Arial, Verdana, Tahoma, helvetica, sans-serif;}

/*------------ CLASES GENÉRICAS ---------------------*/
.hide { display: none;}

/* Estilos CABECERA */
#contenido {width:auto; margin:0 auto; padding:0;}
#contenido .escudo {border:0; margin:0.5em 0 0.2em 0; padding:0; float:left;}

#contenido h1 {display: none}
