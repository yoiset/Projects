package es.dgoj.rgiaj.form;

import org.springframework.beans.factory.annotation.Configurable;
import org.springframework.web.multipart.commons.CommonsMultipartFile;

/**
 * Formulario para ficheros adjuntos.
 * @author dbeltran
 * @version 1.0
 */
@Configurable
public final class AttachmentForm {

	/**
	 * Fichero adjunto.
	 */
	private CommonsMultipartFile fileToUpload;

	public CommonsMultipartFile getFileToUpload() {
		return fileToUpload;
	}

	public void setFileToUpload(CommonsMultipartFile fileToUpload) {
		this.fileToUpload = fileToUpload;
	}

	
	
}
