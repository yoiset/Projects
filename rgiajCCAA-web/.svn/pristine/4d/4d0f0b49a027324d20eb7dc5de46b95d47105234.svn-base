#contenido #interior #central #formulcuad fieldset label {width:auto;}

#contenido #interior #central .listado-acciones-derecha.acciones-tab{
    text-align:right; clear:both; margin:0em 0em 0.1em 0em; padding:0em 0em 0 0.9em;
	border:0; display:block; width:99%; font-family: Arial, Helvetica, sans-serif;
	position:relative; float: right; height:0; top:-24px;}

#contenido #interior #central #formulario fieldset label.linea-dos {position: relative; clear: both; display: block; margin: 0em 0 0em 1em; padding: 0.2em 0 0.2em 0;}
#contenido #interior #central #formulario fieldset label.linea-dos span {color: #000; text-align:left; font-family: Arial, Helvetica, sans-serif; font-size: 0.75em;}

#contenido #interior #central #formulario fieldset label.linea-dos span.label {clear: left; width: 21em; float: left;  padding: 0.1em 0.3em 0 0.2em;}
#contenido #interior #central #formulario fieldset label.linea-dos input {float: left; text-align: left; background-color: #fff; border: 1px #9EB6BA solid; padding-left: 0.2em; margin: 0 0 0 0.4em; font-family: Arial, Helvetica, sans-serif; font-size: 0.75em;}
#contenido #interior #central #formulario fieldset label.linea-dos .file {width: 52em; display: block;}
#contenido #interior #central #formulario fieldset label.linea-dos input.checkbox {float: left; text-align: left; background-color: #fff; border: 0px; padding: 0em 0 0em 0.2em; margin: 0 0 0 0.4em; font-family: Arial, Helvetica, sans-serif; font-size: 0.75em;}
#contenido #interior #central #formulario fieldset label.linea-dos select {float: left; text-align: left; background-color: #fff; border: 1px #9EB6BA solid; padding: 0; margin: 0 0 0 0.4em; font-family: Arial, Helvetica, sans-serif; font-size: 0.75em; }
#contenido #interior #central #formulario fieldset label.linea-dos textarea {width: 52em; height: 6em; float: left; text-align: left; background-color: #fff; border: 1px #9EB6BA solid; padding-left: 0.2em; margin: 0 0 0 0.4em; font-family: Arial, Helvetica, sans-serif; font-size: 0.75em;}
#contenido #interior #central #formulario fieldset label.linea-dos span.date {padding-left: 0.6em;}
#contenido #interior #central #formulario fieldset label.linea-dos .disabled {float: left; text-align: left; background-color: #eee; border: 1px #9EB6BA solid; padding-left: 0.2em; margin: 0 0 0 0.4em; font-family: Arial, Helvetica, sans-serif; font-size: 0.75em;}
#contenido #interior #central #formulario fieldset label.linea-dos a.link {padding-left: 0.6em; font-size: 0.75em;}
#contenido #interior #central #formulario fieldset label.linea-dos .radio {border: 0;} 

#contenido #interior #central #formulcuad fieldset label.linea-medio.linea-dos {
    float: left;
    /*left: 20.4em;*/
    position: relative;
    /*top: -1.45em;*/
	clear: none; display: block; margin: 0em 0em 0em 0em; padding: 0.2em 0 0.2em 0;
}

#contenido #interior #central #formulcuad fieldset label.linea-medio input {width:18em;}
#contenido #interior #central #formulcuad fieldset label.linea-medio input.checkbox {width:1em;}

#contenido #interior #central #formulcuad fieldset label.linea-medio span.label_col1.titulo {display:table-cell; clear: left;  width: 16em;  float: left;  padding: 0.1em 0.3em 0 0.2em;  font-weight: bold;}
#contenido #interior #central #formulcuad fieldset label.linea-medio span.label_col2 {display:table-cell; float:left; width: 16em; padding: 0.1em 0.3em 0 1em;}
#contenido #interior #central #formulcuad fieldset label.linea-medio span.label_col1b {display:table-cell; float:left; width: 11em; padding: 0.1em 0.3em 0 1em;}
#contenido #interior #central #formulcuad fieldset label.linea-medio span.label_col2b {display:table-cell; float:left; width: 11em; padding: 0.1em 0.3em 0 4em;}
#contenido #interior #central #formulcuad fieldset label.linea-medio span.label_nueva {display:table-cell; float:left; width: 60em; padding: 0.1em 0.3em 0 0.2em;}

